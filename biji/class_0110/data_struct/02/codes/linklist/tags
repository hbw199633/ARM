create_linklist	linklist.c	/^linknode * create_linklist(void)$/;"	f
data	linklist.h	/^	datatype 	  data;$/;"	m	struct:node
datatype	linklist.h	/^typedef int datatype;$/;"	t
linknode	linklist.h	/^}linknode;$/;"	t	typeref:struct:node
main	main.c	/^int main(int argc, const char *argv[])$/;"	f
next	linklist.h	/^	struct node * next;$/;"	m	struct:node	typeref:struct:node::node
node	linklist.h	/^typedef struct node {$/;"	s
